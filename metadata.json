{
  "name": "luckyraul-sphinx",
  "version": "1.0.0",
  "author": "Nikita Tarasov",
  "summary": "Module for installing and managing Sphinx Search",
  "license": "Apache-2.0",
  "source": "https://github.com/luckyraul/puppet-sphinx.git",
  "project_page": "https://github.com/luckyraul/puppet-sphinx",
  "issues_url": "https://github.com/luckyraul/puppet-sphinx/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
